#cspec
master_job_name:runtest
trials:1
permute:x=1 4
permute:y=1,2,3

scores_from:file=<permutation_results_dir>/run_test.csv,column_name=auc,row_number=1
scores_to:<output_root>/runtest_out/collected_results
scores_y_axis:y
scores_x_axis:x

#
# permutation-dependent mappings can be expressed like this.  These will be matched by 
# using permutations like this:  <config[(month)]>
#

#
<replace>:root=/nfs/foo/bar
<replace>:x_dir=<root>/(letter)/<config[(letter)]>/(number)
<replace>:algs_dir=/nfs/algs
<replace>:tools_dir=<algs_dir>/tools
<replace>:output_root=/nfs/stak/students/i/irvine/python/cluster/test
#
#  results_dir is where the generated results will be
#
root_results_dir:<output_root>/runtest_out/results
#
#  script is where the generated scripts will be
#
script_dir:<output_root>/runtest_out/scripts
#
qsub_command:-q eecs,eecs1,eecs,share
#qsub_command:-M someone@gmail.com
#qsub_command:-m beas
one_up_basis:0

command:python run_test.python (x) (y) <permutation_results_dir>/run_test.csv
#

