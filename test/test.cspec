#cspec
tag=_myTag
permute:number:1,2,3
permute:letter:AAA,BBB
concise_print:letter,l
concise_print:month,m
#
# permutation-dependent mappings can be expressed like this.  These will be matched by 
# using permutations like this:  <config[(month)]>
#
config[AAA]=aaa
config[BBB]=bbb

pretty[1]=one
pretty[2]=two
pretty[3]=three
#
permutation_set_name=unittest
root=/nfs/foo/bar
x_dir=<root>/(letter)/<config[(letter)]>/(number)
algs_dir=/nfs/algs
tools_dir=<algs_dir>/tools
results_root=/nfs/foo/results/<permutation_set_name>
results_dir=<results_root>/(number)/(letter)
outfile_root=<pretty[(number)]>__TEST
#
#  dir_script is where the generated scripts will be
#
dir_script=./scripts_<permutation_set_name>
#
qsub_command:-q eecs,eecs1,eecs,share
qsub_command:-M someone@gmail.com
qsub_command:-m beas
one_up_basis=100

command:ls -la /nfs/missing > <x_dir>/<pretty[(number)]>.txt
#

